{
	"compilerOptions": {
		"target": "ES6",
		"lib": [
			"DOM",
			"ES6",
			"DOM.Iterable",
			"ScriptHost",
			"ES2016.Array.Include"
		],
		"module": "CommonJS",
		"allowJs": true,
		"rootDir": "src",
		"outDir": "dist",
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		// "sourceMap": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
		"declaration": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}